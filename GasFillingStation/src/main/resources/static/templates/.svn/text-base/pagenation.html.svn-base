<div class="pagenation">
    <span>Show Rows:</span>
    <select ng-change="changePageSize()" ng-model="pageSize">
        <option value=5>5</option>
        <option value=10>10</option>
        <option value=15>15</option>
        <option value=20>20</option>
        <option value=25>25</option>
    </select>
    <span class="goto">Go to</span>
    <input type="text" />
    <span>1-25 of 62</span>
    <img src="/img/kiosks/leftError.png" ng-click="turnPage('next')"/>
    <img src="/img/kiosks/rightError.png" ng-click="turnPage('pre')"/>
</div>